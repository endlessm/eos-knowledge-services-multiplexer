{
    "app-id": "com.endlessm.BaseEknServicesMultiplexer",
    "branch": "@BRANCH@",
    "runtime": "com.endlessm.apps.Platform",
    "runtime-version": "@SDK_BRANCH@",
    "sdk": "com.endlessm.apps.Sdk",
    "add-extensions": {
        "com.endlessm.EknServices3.Extension": {
            "directory": "build/eos-knowledge-services/3",
            "version": "@BRANCH@"
        },
        "com.endlessm.EknServices4.Extension": {
            "directory": "build/eos-knowledge-services/4",
            "version": "@BRANCH@"
        }
    },
    "modules": [
        {
            "name": "multi-eos-knowledge-services-directories",
            "buildsystem": "simple",
            "build-commands": [
                "mkdir -p /app/build/eos-knowledge-services/3",
                "mkdir -p /app/build/eos-knowledge-services/4"
            ]
        },
        {
            "name": "appstream",
            "buildsystem": "simple",
            "build-commands": [
                "install -Dm644 com.endlessm.BaseEknServicesMultiplexer.metainfo.xml /app/share/metainfo/com.endlessm.BaseEknServicesMultiplexer.metainfo.xml"
            ],
            "sources": [
                {
                    "type": "file",
                    "path": "com.endlessm.BaseEknServicesMultiplexer.metainfo.xml"
                }
            ]
        }
    ]
}
